local tweenservice = game:GetService("TweenService")
local marketplaceService = game:GetService("MarketplaceService")
local isSuccessful, gamename = pcall(marketplaceService.GetProductInfo, marketplaceService, game.PlaceId)

local SINHUB = Instance.new("ScreenGui")
local Main = Instance.new("Frame")
local Load = Instance.new("Frame")
local TextLabel = Instance.new("TextLabel")
local EFFECT = Instance.new("Frame")
local UICorner = Instance.new("UICorner")
local TextLabel_2 = Instance.new("TextLabel")

SINHUB.Name = "SIN HUB"
SINHUB.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
SINHUB.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Main.Name = "Main"
Main.Parent = SINHUB
Main.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
Main.BorderSizePixel = 0
Main.Position = UDim2.new(0.411979169, 0, 0.398034394, 0)
Main.Size = UDim2.new(0, 337, 0, 0)

Load.Name = "Load"
Load.Parent = Main
Load.BackgroundColor3 = Color3.fromRGB(255, 128, 0)
Load.BorderSizePixel = 0
Load.Position = UDim2.new(0, 0, 0.963636339, 0)
Load.Size = UDim2.new(0, 0, 0, 6)

TextLabel.Parent = Main
TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
TextLabel.BackgroundTransparency = 1.000
TextLabel.Size = UDim2.new(0, 337, 0, 31)
TextLabel.Font = Enum.Font.SourceSansSemibold
TextLabel.Text = "SIN HUB"
TextLabel.TextColor3 = Color3.fromRGB(255, 128, 0)
TextLabel.TextScaled = true
TextLabel.TextSize = 14.000
TextLabel.TextTransparency = 1.000
TextLabel.TextWrapped = true

EFFECT.Name = "EFFECT"
EFFECT.Parent = TextLabel
EFFECT.BackgroundColor3 = Color3.fromRGB(255, 128, 0)
EFFECT.BackgroundTransparency = 1.000
EFFECT.BorderSizePixel = 0
EFFECT.Position = UDim2.new(0.35014838, 0, 1, 0)
EFFECT.Size = UDim2.new(0, 100, 0, 3)

UICorner.Parent = EFFECT

TextLabel_2.Parent = Main
TextLabel_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
TextLabel_2.BackgroundTransparency = 1.000
TextLabel_2.Position = UDim2.new(0, 0, 0.309090912, 0)
TextLabel_2.Size = UDim2.new(0, 337, 0, 63)
TextLabel_2.Font = Enum.Font.SourceSansSemibold
TextLabel_2.Text = "Game Name"
TextLabel_2.TextColor3 = Color3.fromRGB(255, 85, 0)
TextLabel_2.TextScaled = true
TextLabel_2.TextSize = 14.000
TextLabel_2.TextTransparency = 1.000
TextLabel_2.TextWrapped = true

Main:TweenSize(UDim2.new(0, 337, 0, 165),Enum.EasingDirection.Out,Enum.EasingStyle.Quart,0.75)
wait(0.65)
tweenservice:Create(TextLabel,TweenInfo.new(0.65,Enum.EasingStyle.Quart,Enum.EasingDirection.Out),{TextTransparency = 0;}):Play()
tweenservice:Create(EFFECT,TweenInfo.new(0.65,Enum.EasingStyle.Quart,Enum.EasingDirection.Out),{BackgroundTransparency = 0;}):Play()
wait(0.55)
Load:TweenSize(UDim2.new(0, 337, 0, 6),Enum.EasingDirection.Out,Enum.EasingStyle.Linear,3)
wait(3.5)
TextLabel_2.Text = "Game : "..gamename.Name
tweenservice:Create(TextLabel_2,TweenInfo.new(0.65,Enum.EasingStyle.Quart,Enum.EasingDirection.Out),{TextTransparency = 0;}):Play()
wait(2.5)
tweenservice:Create(TextLabel,TweenInfo.new(0.65,Enum.EasingStyle.Quart,Enum.EasingDirection.Out),{TextTransparency = 1;}):Play()
tweenservice:Create(TextLabel_2,TweenInfo.new(0.65,Enum.EasingStyle.Quart,Enum.EasingDirection.Out),{TextTransparency = 1;}):Play()
tweenservice:Create(EFFECT,TweenInfo.new(0.65,Enum.EasingStyle.Quart,Enum.EasingDirection.Out),{BackgroundTransparency = 1;}):Play()
tweenservice:Create(Load,TweenInfo.new(0.65,Enum.EasingStyle.Quart,Enum.EasingDirection.Out),{BackgroundTransparency = 1;}):Play()
wait(0.65)
Main:TweenSize(UDim2.new(0, 337, 0, 0),Enum.EasingDirection.Out,Enum.EasingStyle.Quart,0.75)
wait(0.75)
SINHUB:Destroy()
